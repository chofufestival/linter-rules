{
  "name": "@chofufestival/linter-rules",
  "version": "1.1.4",
  "repository": {
    "type": "git",
    "url": "https://github.com/chofufestival/linter-rules.git"
  },
  "packageManager": "yarn@4.1.1",
  "exports": {
    "./commitlint": {
      "types": "./commitlint/index.d.ts",
      "import": "./commitlint/index.mjs"
    },
    "./eslint": {
      "types": "./eslint/index.d.ts",
      "import": "./eslint/index.mjs"
    },
    "./eslint/withNext": {
      "types": "./eslint/withNext.d.ts",
      "import": "./eslint/withNext.mjs"
    },
    "./prettier": {
      "types": "./prettier/index.d.ts",
      "import": "./prettier/index.mjs"
    },
    "./stylelint": {
      "types": "./stylelint/index.d.ts",
      "import": "./stylelint/index.mjs"
    },
    "./stylelint/withVue": {
      "types": "./stylelint/index.d.ts",
      "import": "./stylelint/withVue.mjs"
    }
  },
  "scripts": {
    "lint": "eslint --max-warnings 0",
    "format": "prettier --check --ignore-path .gitignore"
  },
  "lint-staged": {
    "./**/*.{js,ts}": [
      "yarn format",
      "yarn lint"
    ]
  },
  "dependencies": {
    "@commitlint/config-conventional": "^19.2.2",
    "@double-great/stylelint-a11y": "^3.0.2",
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^9.1.1",
    "@stylistic/eslint-plugin": "^1.8.0",
    "@stylistic/stylelint-config": "^1.0.1",
    "eslint-config-flat-gitignore": "^0.1.5",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "globals": "^15.1.0",
    "pkg-dir": "^8.0.0",
    "postcss-html": "^1.6.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-sass-guidelines": "^11.1.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-value-no-unknown-custom-properties": "^6.0.1",
    "typescript-eslint": "^7.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/types": "^19.0.3",
    "@types/eslint": "^8.56.10",
    "@types/node": "^20.12.8",
    "cross-env": "^7.0.3",
    "eslint": "^9.1.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "stylelint": "^16.5.0",
    "typescript": "^5.4.5"
  },
  "volta": {
    "node": "20.11.1",
    "yarn": "4.1.1"
  },
  "peerDependencies": {
    "@commitlint/cli": "^19",
    "eslint": "^8",
    "prettier": "^3",
    "stylelint": "^16"
  }
}
